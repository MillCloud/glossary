{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["vite/client", "vite-plugin-pages/client-react"],
    "newLine": "lf",
    "noUnusedLocals": true,
    "baseUrl": ".",
    "sourceMap": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "./**/.*.js",
    "./**/*.js",
    "./**/*.jsx",
    "./**/.*.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.vue"
  ],
  "exclude": ["node_modules", "dist", "dist-ssr", "dist_electron", "out", "unpackage"]
}
