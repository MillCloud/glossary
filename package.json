{
  "name": "glossary",
  "version": "0.7.1",
  "private": true,
  "scripts": {
    "build": "vite build --base=/glossary/",
    "clean": "rimraf ./node_modules/.vite",
    "commit": "cz",
    "dev": "vite --host",
    "postinstall": "pnpm run clean",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint",
    "lint:eslint": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "preview": "vite preview"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/colors": "~6.0.0",
    "@iconify/react": "~3.1.0",
    "@modyqyw/utils": "~2.0.1",
    "ahooks": "~3.0.5",
    "antd": "~4.17.3",
    "js-base64": "~3.7.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-query": "~3.34.4",
    "react-router": "~6.1.1",
    "react-router-dom": "~6.1.1",
    "react-use": "~17.3.1",
    "windicss": "~3.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.16.5",
    "@babel/eslint-parser": "~7.16.5",
    "@commitlint/cli": "~15.0.0",
    "@modyqyw/fabric": "~4.3.0",
    "@types/node": "~17.0.0",
    "@types/react": "~17.0.37",
    "@types/react-dom": "~17.0.11",
    "@typescript-eslint/eslint-plugin": "~5.7.0",
    "@typescript-eslint/parser": "~5.7.0",
    "@vitejs/plugin-react": "~1.1.3",
    "eslint": "~8.5.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.2",
    "markdownlint-cli": "~0.30.0",
    "prettier": "~2.5.1",
    "release-it": "~14.11.8",
    "stylelint": "~14.1.0",
    "typescript": "~4.5.4",
    "vite": "~2.7.3",
    "vite-plugin-pages": "~0.19.4",
    "vite-plugin-windicss": "~1.5.4",
    "vite-tsconfig-paths": "~3.3.17"
  }
}
